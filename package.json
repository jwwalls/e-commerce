{
  "name": "pern-vite",
  "version": "1.0.0",
  "description": "A simple boilerplate for the PERN stack using Vite.js. It includes Node-Postgres v8.8, Express, React 18, React Router DOM v6.6, Morgan, Axios, jsonwebtoken, bcrypt, dotenv, and Nodemon. ",
  "main": "index.js",
  "scripts": {
    "start": "node server/index.js",
    "server:dev": "nodemon server/index.js",
    "start:dev": "(npm run dev --prefix client) & nodemon server/index.js",
    "seed": "node server/db/seed.js"
  },
  "author": "Team Object",
  "license": "ISC",
  "dependencies": {
    "@fullpage/react-fullpage": "^0.1.40",
    "@vitejs/plugin-react": "^4.0.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.0",
    "react-full-page": "^0.1.12",
    "react-fullpage": "^0.1.1",
    "react-router-dom": "^6.11.2",
    "vite": "^4.3.9"
  },
  "devDependencies": {
    "create-vite": "^4.3.1",
    "nodemon": "^2.0.22"
  }
}
